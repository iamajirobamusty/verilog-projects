#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x560b3a6ff0d0 .scope module, "tb_first" "tb_first" 2 3;
 .timescale -9 -12;
v0x560b3a744ed0_0 .net "led", 0 0, L_0x560b3a745040;  1 drivers
v0x560b3a744f70_0 .var "switch", 0 0;
S_0x560b3a6ff250 .scope module, "uut" "first" 2 8, 3 1 0, S_0x560b3a6ff0d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "switch"
    .port_info 1 /OUTPUT 1 "led"
L_0x560b3a745040 .functor BUFZ 1, v0x560b3a744f70_0, C4<0>, C4<0>, C4<0>;
v0x560b3a712f40_0 .net "led", 0 0, L_0x560b3a745040;  alias, 1 drivers
v0x560b3a744db0_0 .net "switch", 0 0, v0x560b3a744f70_0;  1 drivers
    .scope S_0x560b3a6ff0d0;
T_0 ;
    %vpi_call 2 15 "$dumpfile", "led.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x560b3a6ff0d0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560b3a744f70_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560b3a744f70_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560b3a744f70_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560b3a744f70_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 23 "$stop" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x560b3a6ff0d0;
T_1 ;
    %vpi_call 2 28 "$monitor", "Time=%t | switch=%b | led=%b", $time, v0x560b3a744f70_0, v0x560b3a744ed0_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_first.v";
    "first.v";
