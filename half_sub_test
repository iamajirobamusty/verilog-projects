    .port_info 0 /INPUT 1 "A"
    .port_info 1 /INPUT 1 "B"
    .port_info 2 /OUTPUT 1 "D"
    .port_info 3 /OUTPUT 1 "Bout"
L_0x55f0f0237620 .functor XOR 1, v0x55f0f02372c0_0, v0x55f0f0237380_0, C4<0>, C4<0>;
L_0x55f0f0237780 .functor NOT 1, v0x55f0f02372c0_0, C4<0>, C4<0>, C4<0>;
L_0x55f0f02377f0 .functor AND 1, L_0x55f0f0237780, v0x55f0f0237380_0, C4<1>, C4<1>;
v0x55f0f0223e60_0 .net "A", 0 0, v0x55f0f02372c0_0;  1 drivers
v0x55f0f0236ec0_0 .net "B", 0 0, v0x55f0f0237380_0;  1 drivers
v0x55f0f0236f80_0 .net "Bout", 0 0, L_0x55f0f02377f0;  alias, 1 drivers
v0x55f0f0237050_0 .net "D", 0 0, L_0x55f0f0237620;  alias, 1 drivers
v0x55f0f0237110_0 .net *"_s2", 0 0, L_0x55f0f0237780;  1 drivers
    .scope S_0x55f0f02030f0;
T_0 ;
