    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0f02372c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0f0237380_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 17 "$display", "%B %B | %B %B", v0x55f0f02372c0_0, v0x55f0f0237380_0, v0x55f0f0237550_0, v0x55f0f0237450_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0f02372c0_0, 0, 1;
