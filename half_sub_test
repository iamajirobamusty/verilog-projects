S_0x55f0f02030f0 .scope module, "tb_half_sub" "tb_half_sub" 2 1;
 .timescale 0 0;
v0x55f0f02372c0_0 .var "A", 0 0;
v0x55f0f0237380_0 .var "B", 0 0;
